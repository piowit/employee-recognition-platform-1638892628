<%- headers = ['date',
               'email',
               'title',
               'description',
               'price',
               'delivered'] -%>
<%=raw CSV.generate_line headers %>
<%- @orders.each do |order| -%>
  <%=raw CSV.generate_line([order.created_at,
                         order.employee.email, 
                         order.reward_snapshot.title, 
                         order.reward_snapshot.description, 
                         order.reward_snapshot.price,
                         t(order.delivered.to_s)]) -%>
<%- end -%>
